{
  "name": "tall",
  "version": "2.3.0",
  "description": "Promise-based, No-dependency URL unshortner (expander) module for Node.js",
  "main": "lib/index.js",
  "engines": {
    "node": ">=6.14.4"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack -p --env production",
    "release:tag": "git tag $npm_package_version && git push --tags",
    "test:lint": "eslint src/**/*.js",
    "test:jest": "jest --verbose --coverage",
    "test": "npm run test:lint && npm run test:jest",
    "coverage:submit": "codecov"
  },
  "files": [
    "lib/",
    "src/",
    "!src/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmammino/tall.git"
  },
  "author": {
    "name": "Luciano Mammino",
    "email": "lucianomammino@gmail.com",
    "url": "http://loige.co"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lmammino/tall/issues"
  },
  "homepage": "https://github.com/lmammino/tall#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "codecov": "^3.6.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "keywords": [
    "URL",
    "shortner",
    "unshortner",
    "expander",
    "tall",
    "link",
    "expand link",
    "expand url",
    "promise",
    "no depenencies",
    "no dependency"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.4"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
